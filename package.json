    "@types/node": "^20.4.1",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "daisyui": "^3.2.1",
    "electron": "^25.3.1",
    "electron-builder": "^24.6.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.1",
    "nodemon": "^2.0.22",
    "prisma": "^4.16.2",
    "react-router-dom": "^6.14.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
