{
  "name": "kubersee",
  "product_name": "KuberSee",
  "homepage": "./",
  "version": "1.0.0",
  "description": "Kubernetes Visualizer",
  "main": "electron/main.ts",
  "build": {
    "appId": "com.KuberSee.app",
    "nsis": {
      "createDesktopShortcut": "always"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "start": "nodemon server/server.js",
    "electron": "electron ./electron/main.ts",
    "start-server-electron": "concurrently \"npm run start\" \"npm run electron\"",
    "dev": "webpack-dev-server --open & nodemon server/server.js",
    "build": "NODE_ENV=production webpack",
    "dist": "electron-builder"
  },
  "author": "Joey Cheng, Elinor Weissberg, Jordan Lopez, Daniel Zhao, and Alexis Contreras",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
